            bot.send_message(message.chat.id, str("–•–∞—Ä–∫—Ç–µ—Ä: ") + str(random.choice(Harakter)))
            bot.send_message(message.chat.id, str("–î–æ–¥. —ñ–Ω—Ñ–æ: ") + str(random.choice(dopinfo)))
            bot.send_message(message.chat.id, str("–ú–∞—Ç–∏: ") + str(random.choice(profesia)))
            bot.send_message(message.chat.id, str("–ë–∞—Ç—å–∫–æ: ") + str(random.choice(profesia)))
            bot.send_message(message.chat.id, str("üÉè–ö–∞—Ä—Ç–∞ –¥—ñ—ó ‚Ññ1: ") + str(random.choice(diacard)))
            bot.send_message(message.chat.id, str("üÉè–ö–∞—Ä—Ç–∞ –¥—ñ—ó ‚Ññ2: ") + str(random.choice(diacard)))

        elif message.text == 'üõñ–ë—É–Ω–∫–µ—Ä':
            nomer = "–ë—É–Ω–∫–µ—Ä ‚Ññ"
            nomer1 = str(random.randint(0, 999))
            rozdil = "\n"
            locacia = "–ú—ñ—Å—Ü–µ–∑–Ω–∞—Ö–æ–¥–∂–µ–Ω–Ω—è: "
            locate = ["–ê–Ω–∫–∞—Ä–∞, –¢—É—Ä–µ—á—á–∏–Ω–∞", "–ú–æ—Å–∫–≤–∞, –†–æ—Å—ñ—è", "–õ–æ–Ω–¥–æ–Ω, –ë—Ä–∏—Ç–∞–Ω—ñ—è ", "–ö–∏—ó–≤, –£–∫—Ä–∞—ó–Ω–∞", "–ñ–º–µ—Ä–∏–Ω–∫–∞, –£–∫—Ä–∞—ó–Ω–∞",
                      "–¢–µ–≥—É—Å—ñ–≥–∞–ª—å–ø–∞, –ì–æ–Ω–¥—É—Ä–∞—Å", "–í–∞—à–∏–Ω–≥—Ç–æ–Ω, –°–®–ê", "–ê–±—É-–î–∞–±—ñ, –û–ê–ï", "–ë–∞–∫—É, –ê–∑–µ—Ä–±–∞–π–¥–∂–∞–Ω",
                      "–ë—Ä—é—Å–µ–ª—å, –ë–µ–ª—å–≥—ñ—è",
                      "–ë—É—Ö–∞—Ä–µ—Å—Ç, –†—É–º—É–Ω—ñ—è", "–î–µ–ª—ñ, –Ü–Ω–¥—ñ—è", "–ú—ñ–Ω—Å—å–∫, –ë—ñ–ª–æ—Ä—É—Å—ñ—è", "–°–∞–º–±—ñ—Ä, –£–∫—Ä–∞—ó–Ω–∞", "–ü–∞—Ä–∏–∂, –§—Ä–∞–Ω—Ü—ñ—è",
                      "–ë–µ—Ä–ª—ñ–Ω, –ù—ñ–º–µ—á—á–∏–Ω–∞", "–í–∞—Ä—à–∞–≤–∞, –ü–æ–ª—å—â–∞"]
            locate1 = str(random.choice(locate))
            rozmir = "–†–æ–∑–º—ñ—Ä: "

            rozmir1 = str(random.randint(30, 800))
            izza = "–á–∂—ñ –¥–æ—Å—Ç–∞—Ç–Ω—å–æ –Ω–∞: "
            izza1 = str(random.randint(1, 36))
            izzapoz = " –º—ñ—Å."
            vorog = ["–î–∞–ª–µ–∫–æ", "1 –∫–º", "5 –∫–º", "10 –∫–º", "20 –∫–º", "50 –∫–º", "100 –∫–º", "500 –º", "–ù–µ–≤—ñ–¥–æ–º–æ"]
            vorogloc = str(random.choice(vorog))
            vorog1 = "–î–æ –≤–æ—Ä–æ–∂–æ–≥–æ –±—É–Ω–∫–µ—Ä–∞: "
            v = nomer + nomer1 + rozdil + locacia + locate1 + rozdil + rozmir + rozmir1 + "m¬≤" + rozdil + izza + izza1 \
                + izzapoz + rozdil + vorog1 + vorogloc
            bot.send_message(message.chat.id, v)
        elif message.text == 'üåã–Ü—Å—Ç–æ—Ä—ñ—è':
            story1 = "      –ü–ª–∞–Ω ¬´–°—É–ø–µ—Ä-–ø—É–ø–µ—Ä —Å–µ–ª–æ¬ª\n \
–ü'—è—Ç–∏–∫–ª–∞—Å–Ω–∏–∫ –¥—É–∂–µ –∑–∞—Ö–æ–ø–∏–≤—Å—è —é—Ç—É–± –∫–∞–Ω–∞–ª–æ–º Slivki show \
—ñ —Å—Ç–≤–æ—Ä–∏–≤ —É —Å–≤–æ—î–º—É –ø—ñ–¥–≤–∞–ª—ñ —è–¥–µ—Ä–Ω—É –±–æ–º–±—É, —Ö–æ—á–∞ \
–ø–ª–∞–Ω—É–≤–∞–≤ –ª–∏—à–µ –∑—Ä–æ–±–∏—Ç–∏ –ª—ñ—Ç–∞—á–æ–∫ –∑ –ø–∞–ø–µ—Ä—É.\n\
–ù–µ –ø—ñ–¥–æ–∑—Ä—é—é—á–∏ –¥–æ –∫—ñ–Ω—Ü—è, —á–∏–º –≤—Å–µ –º–æ–∂–µ –∑–∞–∫—ñ–Ω—á–∏—Ç–∏—Å—å, \
–≤—ñ–Ω –≤–∏—Ç—è–≥—É—î –ø—ñ–¥ —á–∞—Å —É—Ä–æ–∫—É —Å–≤–æ—é —Å–∞–º–æ—Ä–æ–±–∫—É \
—ñ –≤–∏–ø–∞–¥–∫–æ–≤–æ –∑–∞–ø—É—Å–∫–∞—î –±–æ–º–±—É\n\
–ë—É—É–º\n\
–í—Å—å–æ\n\
–í–µ—Å—å —Å–≤—ñ—Ç –∑–Ω–∏—â–µ–Ω–æ, –∞–ª–µ –≤–∞–º –ø–æ—â–∞—Å—Ç–∏–ª–æ \
–≤—Ä—è—Ç—É–≤–∞—Ç–∏—Å—å —É –ø—Ä–µ–∫—Ä–∞—Å–Ω–æ–º—É —Å–µ–ª—ñ.\n\
–í–∏—Ä—ñ—à—ñ—Ç—å, —Ö—Ç–æ –∑–∞–ª–∏—à–µ—Ç—å—Å—è –≤ –∂–∏–≤–∏—Ö"

            story2 = """      –ü–ª–∞–Ω ¬´–ü–µ—Ä–µ—Ö–≤–∞—Ç¬ª \n\
